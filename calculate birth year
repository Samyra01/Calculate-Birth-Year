name = input("Qual é o seu nome? ").strip().title()
idade = int(input("Qual é a sua idade? "))

ano_nascimento = int((idade) - 2023)

if ano_nascimento >= 2005:
        print(f"{name}, você é menor de idade e nasceu no ano {ano_nascimento}")
else:
    print(f"{name}, você é maior de idade e nasceu no ano {ano_nascimento}")
